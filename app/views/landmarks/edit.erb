<form method="POST" action="/landmarks/<%=@landmark.id%>">
  <input id="hidden" type="hidden" name="_method" value="patch">
<label><h4>Create New Landmark:</h4></label>
 <label>Name:</label>
  <input type="text" id="name" name="landmark[name]"><%=@landmark.name %>
  <label>Year:</label>
  <input type="text" id="year_completed" name="landmark[year_completed]"><%= @landmark.year_completed %>
  <input type="submit" value="Edit Landmark">
</form>